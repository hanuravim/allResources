{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualNetworkName": {
            "type": "String"
        },
        "subnet_Names": {
            "type": "object"
        },
        "address_Prefixes": {
            "type": "object"
        },
        "nsgName": {
            "type": "object"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/virtualNetworks",
            "name": "[parameters('virtualNetworkName')]",
            "apiVersion": "2018-10-01",
            "location": "[resourceGroup().location]",
            "scale": null,
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "[parameters('address_Prefixes').vnet]"
                    ]
                },
                "subnets": [
                    {
                        "name": "[parameters('subnet_Names').appgateway]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').appgateway]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').appgateway)]"
                            },
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').gateway]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').gateway]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').gateway)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').worker]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').worker]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').worker)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').webapi]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').webapi]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').webapi)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').vpn]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').vpn]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').vpn)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').sql]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').sql]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').sql)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').jumpbox]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').jumpbox]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').jumpbox)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').sftp]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').sftp]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').sftp)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').wasabi]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').wasabi]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').wasabi)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').tableau]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').tableau]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').tableau)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').redis]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').redis]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').redis)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').loganalytics]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').loganalytics]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').loganalytics)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names').ad]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes').ad]",
                            "networkSecurityGroup": {
                                "id": "[resourceId(resourcegroup().name, 'Microsoft.Network/networkSecurityGroups', parameters('nsgName').ad)]"
                            },
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    }
                ],
                "virtualNetworkPeerings": [],
                "enableDdosProtection": false,
                "enableVmProtection": false
            },
            "dependsOn": []
        }
    ]
}
