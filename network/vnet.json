{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualNetwork_name": {
            "defaultValue": "testemsvnet",
            "type": "String"
        },
        "subnet_Names": {
            "type": "array",
            "defaultValue": [
                "AD",
                "sql",
                "manager",
                "worker",
                "webapi",
                "jumpbox",
                "sftp",
                "wasabi",
                "gateway",
                "vpn",
                "logAnalytics"
            ]
        },
        "address_Prefixes": {
            "type": "array",
            "defaultValue": [
                "10.4.0.0/24",
                "10.4.1.0/24",
                "10.4.2.0/24",
                "10.4.3.0/24",
                "10.4.4.0/24",
                "10.4.5.0/24",
                "10.4.6.0/24",
                "10.4.7.0/24",
                "10.4.8.0/24",
                "10.4.9.0/24",
                "10.4.10.0/24"
            ]
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Network/virtualNetworks",
            "name": "[parameters('virtualNetwork_name')]",
            "apiVersion": "2018-10-01",
            "location": "[resourceGroup().location]",
            "scale": null,
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "10.4.0.0/16"
                    ]
                },
                "subnets": [
                    {
                        "name": "[parameters('subnet_Names')[0]]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes')[0]]",
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names')[1]]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes')[1]]",
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names')[2]]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes')[2]]",
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names')[3]]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes')[3]]",
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names')[4]]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes')[4]]",
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names')[5]]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes')[5]]",
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names')[6]]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes')[6]]",
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names')[7]]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes')[7]]",
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names')[8]]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes')[8]]",
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names')[9]]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes')[9]]",
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    },
                    {
                        "name": "[parameters('subnet_Names')[10]]",
                        "properties": {
                            "addressPrefix": "[parameters('address_Prefixes')[10]]",
                            "serviceEndpoints": [],
                            "delegations": []
                        }
                    }
                ],
                "virtualNetworkPeerings": [],
                "enableDdosProtection": false,
                "enableVmProtection": false
            },
            "dependsOn": []
        }
    ]
}
